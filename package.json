{
  "name": "pebble-mesh",
  "author": "David Peer",
  "version": "1.0",
  "keywords": [
    "clock",
    "minimal",
    "mesh",
    "dark"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Pebble Mesh",
    "uuid": "556723f4-387d-48f8-9318-419a041222d5",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "capabilities": [
      "configurable"
    ],
    "targetPlatforms": [
      "aplite",
      "basalt",
      "diorite"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "WEATHER_TEMPERATURE",
      "WEATHER_LOCATION",
      "WEATHER_REQUEST",
      "WEATHER_LOCATION_CONFIG"
    ],
    "resources": {
      "media": [
        {
          "type": "bitmap",
          "name": "IMAGE_STEP",
          "file": "images/step.png"
        }
      ]
    }
  }
}
